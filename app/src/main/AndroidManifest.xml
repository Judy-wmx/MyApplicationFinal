        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.acer.myapplication.AppActivity"></activity>
        <activity android:name="com.example.acer.myapplication.MainActivity" />
        <activity android:name="com.example.acer.myapplication.NoteActivity" />
        <activity android:name="com.example.acer.myapplication.LoginActivity" />
        <activity android:name="com.example.acer.myapplication.EmotionActivity" />
        <activity
            android:name="com.example.acer.myapplication.WelcomeActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.acer.myapplication.FindActivity" />
        <activity android:name="com.example.acer.myapplication.RegisterActivity" />
        <activity android:name="com.example.acer.myapplication.NewsDisplayActivity" />
        <activity android:name="com.example.acer.myapplication.EditorActivity" />
        <activity android:name="com.example.acer.myapplication.MeActivity" />
        <activity android:name="com.example.acer.myapplication.AnquanyuyinsiActivity" />
